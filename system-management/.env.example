# 应用协议名称（用于URL scheme）
APP_PROTOCOL= # default: meta-note
WEBSOCKET_PORT= # default: 5050
WEB_SERVER_PORT= # default: 5051
DEV_SERVER_PORT= # default: 5173
WINDOW_WIDTH= # default: 1200
WINDOW_HEIGHT= # default: 800
NODE_ENV= # default: production
PROXY_URL= # optional, for Chatgpt, example: http://localhost:7890

# 语音服务 API 地址
VITE_VOICE_API_URL= # default: http://localhost:8000

# Kafka configuration
KAFKA_BROKER= # default: localhost:9092

# Redis configuration
REDIS_HOST= # default: localhost
REDIS_PORT= # default: 6379
REDIS_PASSWORD= # optional

# Study Machine Limits
STUDY_LIST_LIMIT_COUNT= # Daily study count limit, default=10
STUDY_LIMIT_TIME= # Daily study time limit in minutes, default=45

# Local LLM Provider (Optional)
# 用于处理语音转文字后的文本理解
# example: http://localhost:11434 (Ollama)